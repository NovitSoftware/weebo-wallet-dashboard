		sourceColumn: dwh Dim_Afiliado[Distrito]

		annotation SummarizationSetBy = Automatic

	column TotalStickersActivos
		summarizeBy: sum
		isNameInferred
		sourceColumn: [TotalStickersActivos]

		annotation SummarizationSetBy = Automatic

