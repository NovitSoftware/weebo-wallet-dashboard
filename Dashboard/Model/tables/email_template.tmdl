		summarizeBy: sum
		sourceColumn: idtemplate

		annotation SummarizationSetBy = Automatic

	column fkcliente
		dataType: int64
		formatString: 0
		sourceProviderType: int
		summarizeBy: sum
		sourceColumn: fkcliente

		annotation SummarizationSetBy = Automatic

	column body
		dataType: string
		sourceProviderType: text
		summarizeBy: none
		sourceColumn: body

		annotation SummarizationSetBy = Automatic

	column titulo
		dataType: string
		sourceProviderType: varchar(100)
		summarizeBy: none
		sourceColumn: titulo

		annotation SummarizationSetBy = Automatic

	column tipo
		dataType: string
		sourceProviderType: varchar(20)
		summarizeBy: none
		sourceColumn: tipo

		annotation SummarizationSetBy = Automatic

	column audience_id
		dataType: string
		sourceProviderType: varchar(100)
		summarizeBy: none
		sourceColumn: audience_id

		annotation SummarizationSetBy = Automatic

	partition email_template = m
		mode: import
		source =
				let
				    Source = Sql.Databases("40.79.18.177"),
				    Apployalty = Source{[Name="Apployalty"]}[Data],
				    dbo_email_template = Apployalty{[Schema="dbo",Item="email_template"]}[Data]
				in
				    dbo_email_template

